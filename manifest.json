{
	"manifest_version": 2,
	"name": "Not OSSP",
	"description": "Adds warnings to posts in the One Sentence Startup Pitches (OSSP) Facebook group if they are more than one sentence long.",
	"version": "1.0.1",
	"content_scripts": [
		{
			"matches": ["https://*.facebook.com*"],
			"css": ["main.css"],
			"js": ["jquery-3.1.0.slim.min.js", "nlp_compromise.min.js", "notOSSP.js"]
		}
	],
	"icons": {
		"128": "icon-128.png",
		"48": "icon-48.png"
	},
	"homepage_url": "https://github.com/kz26/NotOSSP"
}
